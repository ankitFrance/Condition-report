  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
    integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
    integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />

</head>

<body style="background-color: #E8E9EB;">



  <!------------------------------------------------------------------------------------------------------------------------------->

  <div class="container">


    <!--**********************************************REPORTING INFORMATION******************************************************-->

 
   <% if (Constatdocument.Reporting_Information && Object.values(Constatdocument.Reporting_Information).some(value => value)) { %>
     <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">REPORTING INFORMATION</h2>
       <ul style="list-style-type: none;">
         <% Object.entries(Constatdocument.Reporting_Information).forEach(([key, value]) => { %>
          <% if (value) { %>
          <li><strong><%= key %>:</strong> <%= value %></li>
          <% } %>
         <% }) %>
       </ul>
   <% } %>



<!--**********************************************OBJECT IDENTIFICATION******************************************************-->


    <% if (Constatdocument.Object_identification && Object.values(Constatdocument.Object_identification).some(value => value)) { %>
      <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">OBJECT IDENTIFICATION</h2>
      <ul style="list-style-type: none;">
        <% Object.entries(Constatdocument.Object_identification).forEach(([key, value]) => { %>
          <% if (value) { %>
            <li><strong><%= key %>:</strong> <%= value %></li>
          <% } %>
        <% }) %>
      </ul>
    <% } %>
    

<!--------------------- OBJECT DESCRIPTION---------------------------------------------------------------------------------------------->


<% if (Constatdocument.Object_description && Object.values(Constatdocument.Object_description).some(value => value)) { %>
  <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">OBJECT DESCRIPTION</h2>
  <ul style="list-style-type: none;">
    <% Object.entries(Constatdocument.Object_description).forEach(([key, value]) => { %>
     
      <% if (value !== undefined && value !== null && (value !== '' || (Array.isArray(value) && value.length > 0))) { %>
        <% if (!(Array.isArray(value) && value.length === 0)) { %>
           <li><strong><%= key %>:</strong> <%= value %></li>
        <% } %>
      <% } %>
   
    <% }) %>
  </ul>
<% } %>


<!--------------------- OBJECT ENVIRONMENT---------------------------------------------------------------------------------------------->


<% if (Constatdocument.Object_environment && Object.values(Constatdocument.Object_environment).some(value => value)) { %>
  <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">OBJECT ENVIRONMENT</h2>
  <ul style="list-style-type: none;">
    <% Object.entries(Constatdocument.Object_environment).forEach(([key, value]) => { %>
      <% if (value) { %>
        <li><strong><%= key %>:</strong> <%= value %></li>
      <% } %>
    <% }) %>
  </ul>
<% } %>


<!--------------------- CONDITION DESCRIPTION---------------------------------------------------------------------------------------------->


<% if (Constatdocument.Conditions_description && Object.values(Constatdocument.Conditions_description).some(value => value)) { %>
  <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">CONDITION DESCRIPTION</h2>
  <ul style="list-style-type: none;">
    <% Object.entries(Constatdocument.Conditions_description).forEach(([key, value]) => { %>
     
      <% if (value !== undefined && value !== null && (value !== '' || (Array.isArray(value) && value.length > 0))) { %>
        <% if (!(Array.isArray(value) && value.length === 0)) { %>
           <li><strong><%= key %>:</strong> <%= value %></li>
        <% } %>
      <% } %>
   
    <% }) %>
  </ul>
<% } %>


<!--------------------- DIAGNOSTIC AND RECOMMENDATION---------------------------------------------------------------------------------------------->


<% if (Constatdocument.Diagnostic_and_recommendations && Object.values(Constatdocument.Diagnostic_and_recommendations).some(value => value)) { %>
  <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">DIAGNOSTIC AND RECOMMENDATIONS</h2>
  <ul style="list-style-type: none;">
    <% Object.entries(Constatdocument.Diagnostic_and_recommendations).forEach(([key, value]) => { %>
      <% if (value) { %>
        <li><strong><%= key %>:</strong> <%= value %></li>
      <% } %>
    <% }) %>
  </ul>
<% } %>


<!------------------------------------------------------------------------------------------------------------------------------------------>  


  </div>


