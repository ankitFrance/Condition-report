<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    
</head>

<body style="background-color: #E8E9EB;">
  

  <div style="text-align: center; padding: 15px;">
    <button class="btn btn-primary" onclick="printPage()">
        <i class="fas fa-print"></i> Print
    </button>
  </div>

<!------------------------------------------------------------------------------------------------------------------------------->
   
<div class="container">


<!--**********************************************REPORTING INFORMATION******************************************************-->

   <div style="position: relative; text-align: center;" >

    <h5 style="color: yellow; background-color: purple; padding: 10px; margin-bottom: 0;"> 
      REPORTING INFORMATION 
    </h5> 
   
  </div>
     
  <div class="container" style="background-color: white;"> 

    <% if (reportDocument.Reporting_Information) { %>
      <div class="row">
        <% let count = 0; %>
        <% Object.entries(reportDocument.Reporting_Information).forEach(([key, value]) => { %>
          <% if (value) { %>
            <% if (count < 7) { %>
              <div class="col-md-6">
                <p><strong><%= key %>:</strong> <%= value %></p>
              </div>
            <% } else { %>
              <div class="col-md-6">
                <p><strong><%= key %>:</strong> <%= value %></p>
              </div>
            <% } %>
            <% count++; %>
          <% } %>
        <% }) %>
      </div>
    <% } %>

  </div>

<!--**********************************************OBJECT IDENTIFICATION******************************************************-->

 <div style="position: relative; text-align: center;" >

    <h5 style="color: yellow; background-color: purple; padding: 10px; margin-bottom: 0;"> 
    OBJECT IDENTIFICATION
    </h5> 
  
 </div>

    <div class="container" style="background-color: white;"> 

    <% if (reportDocument.Object_identification) { %>
      <div class="row">
        <% let count = 0; %>
        <% Object.entries(reportDocument.Object_identification).forEach(([key, value]) => { %>
          <% if (value) { %>
            <% if (count < 7) { %>
              <div class="col-md-6">
                <p><strong><%= key %>:</strong> <%= value %></p>
              </div>
            <% } else { %>
              <div class="col-md-6">
                <p><strong><%= key %>:</strong> <%= value %></p>
              </div>
            <% } %>
            <% count++; %>
          <% } %>
        <% }) %>
      </div>
    <% } %>
  
    </div>
  
<!--**********************************************OBJECT DESCRIPTION******************************************************-->
  
  <div style="position: relative; text-align: center;" >

    <h5 style="color: yellow; background-color: purple; padding: 10px; margin-bottom: 0;"> 
    OBJECT DESCRIPTION 
    </h5> 
   
  </div>

    <div class="container" style="background-color: white;"> 

      <% if (reportDocument.Object_description && reportDocument.Object_description.images && reportDocument.Object_description.images.length > 0) { %>
        <% Object.entries(reportDocument.Object_description).forEach(([key, value]) => { %>
          <% if ( key !== 'filePaths'  && !(Array.isArray(value) && value.length === 0)) { %>
            <div class="col-md-6">
              <p><strong><%= key %>:</strong> <%= value %></p>
            </div>
          <% } %>
        <% }) %>
        <% reportDocument.Object_description.images.forEach(image => { %>
          <img src="<%= image %>" alt="Image" class="img-fluid" style="height: 300px; width: 300px">
        <% }) %>
      
      <% } else { %>
        <!-- Display the content of Object_description -->
        <% if (reportDocument.Object_description) { %>
          <div class="row">

            <% Object.entries(reportDocument.Object_description).forEach(([key, value]) => { %>
              <% if (value) { %>
                  <p><strong><%= key %>:</strong> <%= value %></p>
              <% } %>
            <% }) %>

          </div>
        <% } %>
      <% } %>
   
    </div>

<!--**********************************************OBJECT ENVIRONMENT**************************************************-->

<div style="position: relative; text-align: center;" >

    <h5 style="color: yellow; background-color: purple; padding: 10px; margin-bottom: 0;"> 
    OBJECT ENVIRONMENT
    </h5> 
   
</div>
    <div class="container" style="background-color: white;"> 
     

      <% if (reportDocument.Object_environment) { %>
        <div class="row">
              <!-- Iterate over each property of Reporting_Information and display only if value is not empty -->
              <% Object.entries(reportDocument.Object_environment).forEach(([key, value]) => { %>
                  <% if (value) { %>
                      <p><strong><%= key %>:</strong> <%= value %></p>
                  <% } %>
              <% }) %>
            </div>
      <% } %>
      
     
    </div>
    
 <!--*************************************************CONDITION DESCRIPTION**************************************************-->   
  
    <div style="position: relative; text-align: center;" >

    <h5 style="color: yellow; background-color: purple; padding: 10px; margin-bottom: 0;"> 
    CONDITIONS DESCRIPTION (OBJECT)
    </h5> 
   
    </div>

    <div class="container" style="background-color: white;"> 

      <% if (reportDocument.Conditions_description && reportDocument.Conditions_description.images2 && reportDocument.Conditions_description.images2.length > 0) { %>
        <% Object.entries(reportDocument.Conditions_description).forEach(([key, value]) => { %>
          <% if ( key !== 'filePaths'  && !(Array.isArray(value) && value.length === 0)) { %>
            <div class="col-md-6">
              <p><strong><%= key %>:</strong> <%= value %></p>
            </div>
          <% } %>
        <% }) %>
        <% reportDocument.Conditions_description.images2.forEach(image => { %>
          <img src="<%= image %>" alt="Image" class="img-fluid" style="height: 300px; width: 300px">
        <% }) %>
      
      <% } else { %>
        <!-- Display the content of Object_description -->
        <% if (reportDocument.Conditions_description) { %>
          <div class="row">

            <% Object.entries(reportDocument.Conditions_description).forEach(([key, value]) => { %>
              <% if (value) { %>
                  <p><strong><%= key %>:</strong> <%= value %></p>
              <% } %>
            <% }) %>
            
          </div>
        <% } %>
      <% } %>

    </div>


 <!--**********************************************DIAGNOSTIC AND RECOMMENDATION***********************************************--> 

     <div style="position: relative; text-align: center;" >

        <h5 style="color: yellow; background-color: purple; padding: 10px; margin-bottom: 0;"> 
         DIAGNOSTIC AND RECOMMENDATIONS
       </h5> 
   
    </div>

    <div class="container" style="background-color: white;"> 

      <% if (reportDocument.Diagnostic_and_recommendations) { %>
        <div class="row">
              <!-- Iterate over each property of Reporting_Information and display only if value is not empty -->
              <% Object.entries(reportDocument.Diagnostic_and_recommendations).forEach(([key, value]) => { %>
                  <% if (value) { %>
                      <p><strong><%= key %>:</strong> <%= value %></p>
                  <% } %>
              <% }) %>
            </div>
      <% } %>

    </div>
 <!--***************************************************************************************************************************-->

  </div>

 <!------------------------------------------------------------------------------------------------------------------------------->
   



<script src="https://cdn.jsdelivr.net/npm/js-confetti@latest/dist/js-confetti.browser.js"></script>
