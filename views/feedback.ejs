<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    
</head>

<body style="background-color: #E8E9EB;">
  

  <div style="text-align: center; padding: 15px;">
    <button class="btn btn-primary" onclick="printPage()">
        <i class="fas fa-print"></i> Print
    </button>
  </div>

<!--*****************************************************************************************************************************************-->
   
<div class="container" style="background-color: white;">


  <h1 style="text-align: center;">Condition Report</h1>

 




<!--------------------- REPORTING INFORMATION---------------------------------------------------------------------------------------------->

<% if (reportDocument.Reporting_Information && Object.values(reportDocument.Reporting_Information).some(value => value)) { %>
  <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">REPORTING INFORMATION</h2>
  <ul style="list-style-type: none;">
    <% Object.entries(reportDocument.Reporting_Information).forEach(([key, value]) => { %>
      <% if (value) { %>
        <li><strong><%= key %>:</strong> <%= value %></li>
      <% } %>
    <% }) %>
  </ul>
<% } %>


<!------------------------------------------------------------------------------------------------------------------------------------------>   
 
<!--------------------- OBJECT IDENTIFICATION---------------------------------------------------------------------------------------------->

<% if (reportDocument.Object_identification && Object.values(reportDocument.Object_identification).some(value => value)) { %>
  <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">OBJECT IDENTIFICATION</h2>
  <ul style="list-style-type: none;">
    <% Object.entries(reportDocument.Object_identification).forEach(([key, value]) => { %>
      <% if (value) { %>
        <li><strong><%= key %>:</strong> <%= value %></li>
      <% } %>
    <% }) %>
  </ul>
<% } %>


<!------------------------------------------------------------------------------------------------------------------------------------------>   

<!--------------------- OBJECT DESCRIPTION---------------------------------------------------------------------------------------------->

<% if (reportDocument.Object_description && Object.values(reportDocument.Object_description).some(value => value)) { %>
  <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">OBJECT DESCRIPTION</h2>
  <ul style="list-style-type: none;">
    <% Object.entries(reportDocument.Object_description).forEach(([key, value]) => { %>
     
      <% if (value !== undefined && value !== null && (value !== '' || (Array.isArray(value) && value.length > 0))) { %>
        <% if (!(Array.isArray(value) && value.length === 0)) { %>
           <li><strong><%= key %>:</strong> <%= value %></li>
        <% } %>
      <% } %>
   
    <% }) %>
  </ul>
<% } %>

<% if (images && images.length > 0) { %>
  <div class="row">
    <div class="col-md-6">
      <% images.forEach(image => { %>
        <img src="<%= image %>" alt="Image" class="img-fluid" style="height: 300px; width: 300px">
      <% }) %>
    </div>
  </div>
<% } %>


<!------------------------------------------------------------------------------------------------------------------------------------------>   

<!--------------------- OBJECT ENVIRONMENT---------------------------------------------------------------------------------------------->

<% if (reportDocument.Object_environment && Object.values(reportDocument.Object_environment).some(value => value)) { %>
  <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">OBJECT ENVIRONMENT</h2>
  <ul style="list-style-type: none;">
    <% Object.entries(reportDocument.Object_environment).forEach(([key, value]) => { %>
      <% if (value) { %>
        <li><strong><%= key %>:</strong> <%= value %></li>
      <% } %>
    <% }) %>
  </ul>
<% } %>


<!------------------------------------------------------------------------------------------------------------------------------------------>   

<!--------------------- CONDITION DESCRIPTION---------------------------------------------------------------------------------------------->


<% if (reportDocument.Conditions_description && Object.values(reportDocument.Conditions_description).some(value => value)) { %>
  <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">CONDITION DESCRIPTION</h2>
  <ul style="list-style-type: none;">
    <% Object.entries(reportDocument.Conditions_description).forEach(([key, value]) => { %>
     
      <% if (value !== undefined && value !== null && (value !== '' || (Array.isArray(value) && value.length > 0))) { %>
        <% if (!(Array.isArray(value) && value.length === 0)) { %>
           <li><strong><%= key %>:</strong> <%= value %></li>
        <% } %>
      <% } %>
   
    <% }) %>
  </ul>
<% } %>

<% if (images2 && images2.length > 0) { %>
  <div class="row">
    <div class="col-md-6">
      <% images2.forEach(image => { %>
        <img src="<%= image %>" alt="Image" class="img-fluid" style="height: 300px; width: 300px">
      <% }) %>
    </div>
  </div>
<% } %>



<!------------------------------------------------------------------------------------------------------------------------------------------>   

<!--------------------- DIAGNOSTIC AND RECOMMENDATION---------------------------------------------------------------------------------------------->

<% if (reportDocument.Diagnostic_and_recommendations && Object.values(reportDocument.Diagnostic_and_recommendations).some(value => value)) { %>
  <h2 style="border-bottom: 2px solid #03346F; color: #03346F;">DIAGNOSTIC AND RECOMMENDATIONS</h2>
  <ul style="list-style-type: none;">
    <% Object.entries(reportDocument.Diagnostic_and_recommendations).forEach(([key, value]) => { %>
      <% if (value) { %>
        <li><strong><%= key %>:</strong> <%= value %></li>
      <% } %>
    <% }) %>
  </ul>
<% } %>


<!------------------------------------------------------------------------------------------------------------------------------------------>   

</div>

<!--*****************************************************************************************************************************************-->
   



